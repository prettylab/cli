{
  "name": "@prettylab/cli",
  "version": "0.1.0",
  "description": "Command-line tool to install and update prettylab dependencies",
  "type": "module",
  "bin": {
    "pretty": "dist/cli.js"
  },
  "engines": { "node": ">=18" },
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsup src/cli.ts --format esm,cjs --dts --target node18 --out-dir dist",
    "dev": "tsx src/cli.ts",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "execa": "^9.4.0",
    "kleur": "^4.1.5",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@types/prompts": "^2.4.9",
    "prettier": "^3.6.2",
    "tsup": "^8.0.1",
    "tsx": "^4.16.0",
    "typescript": "^5.6.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.17.1"
}
